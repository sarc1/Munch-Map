<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.munch_map.BarangayController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <HBox spacing="10">
            <children>
                <ComboBox fx:id="comboBox" onAction="#onComboBoxAction">
                    <items>
                        <FXCollections fx:factory="observableArrayList">
                            <String fx:value="Option 1" />
                            <String fx:value="Option 2" />
                            <String fx:value="Option 3" />
                        </FXCollections>
                    </items>
                </ComboBox>
                <Button fx:id="openButton" text="Open Window" onAction="#openNewWindow" />
            </children>
        </HBox>
    </children>
</AnchorPane>
